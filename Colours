#include <iostream>
using namespace std;

void permutations(string culori[], int n = 7, int index = 0)
{   
    for(int j = 0; j < 6; j++)
    for(int i = 1; i < 2; i++)
    {   if(colours[index] != colours[j] && colours[1] != colours[j])
        cout<<colours[index]<<" "<<colours[1]<<" "<<colours[j]<<endl;
    }
    
    for(int j = 0; j < 6; j++)
    for(int i = 1; i < 2; i++)
    {   if(colours[index] != colours[j] && colours[1] != colours[j])
        cout<<colours[index]<<" "<<colours[3]<<" "<<colours[i]<<endl;;
    }
    if(index < 6)
    permutations(colours,7,index+1);
}
int main()
{
    string colours[] = {"white", "yellow", "red", "green", "blue","black"};
    permutatii(colours);
}
